<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5">
    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>ListServlet</servlet-name>
        <servlet-class>com.ts.mybatis02.servlet.ListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListServlet</servlet-name>
        <url-pattern>/ListServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ListServlet2</servlet-name>
        <servlet-class>com.ts.mybatis02.servlet.ListServlet2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListServlet2</servlet-name>
        <url-pattern>/ListServlet2</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DeleteOneServlet</servlet-name>
        <servlet-class>com.ts.mybatis02.servlet.DeleteOneServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteOneServlet</servlet-name>
        <url-pattern>/DeleteOneServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DeleteBatchServlet</servlet-name>
        <servlet-class>com.ts.mybatis02.servlet.DeleteBatchServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteBatchServlet</servlet-name>
        <url-pattern>/DeleteBatchServlet.action</url-pattern>
    </servlet-mapping>
</web-app>